{
    "name": "edwin-luijten/oauth2-strava",
    "description": "Strava OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Edwin Luijten",
            "email": "edwin.luijten@live.nl"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "strava"
    ],
    "require": {
        "php": "^5.6|| ^7.0 || ^8.0",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0 || ^9.5",
        "mockery/mockery": "^1.3.5",
        "php-parallel-lint/php-parallel-lint": "^1.3.1",
        "squizlabs/php_codesniffer": "^2.3 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "League\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
